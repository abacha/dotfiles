[user]
	name = Adriano Bacha
	email = abacha@gmail.com
[color]
	ui = auto
[github]
	user = abacha
	token = $GITHUB_TOKEN
[merge]
  tool = vim
[diff]
  tool = vimdiff
[dude]
	interval = 60
[alias]
  a = add
  amend = commit --amend
  ap = add --patch
  b = branch
  ci = commit -m
  co = checkout
  dc = diff --cached
  di = diff
  fp = fetch --prune
  h = log --graph --pretty=\"format:%C(yellow)%h%Cblue%d%Creset %s %C(white) %an, %ar%Creset\" --all
  l = log --pretty=\"format:%C(yellow)%h%Cblue%d%Creset %s %C(white) %an, %ar%Creset\" --all
  rb = rebase
  rbm = rebase origin/master
  s = status -s
  st = status
  pop = stash pop
[push]
	default = matching
[branch]
	autosetuprebase = always
[rebase]
  autosquash = true
[pull]
	rebase = true
